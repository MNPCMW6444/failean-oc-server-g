{
  "name": "failean-ocserver",
  "version": "2.0.4",
  "scripts": {
    "v..": "npm version patch --no-git-tag-version",
    "v.": "npm version minor --no-git-tag-version",
    "v": "npm version major --no-git-tag-version",
    "clean:all": "rm -rf node_modules build package-lock.json",
    "clean:prod": "rm -rf node_modules package-lock.json",
    "update": "npx npm-check-updates -u && npm i",
    "build": "tsc",
    "start": "npm run udev",
    "prod": "npm run update && npm run build",
    "dev:fullclean": "npm run clean:all && npm run dev:fullfast",
    "dev:fullfast": "npm run update && npm run dev:fast",
    "dev:fast": "nodemon",
    "dev": "npm run dev:fast",
    "udev": "npm run dev:fullfast"},
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "dependencies": {
    "@aws-sdk/client-servicediscovery": "^3.414.0",
    "@failean/mongo-models": "^1.6.129",
    "@failean/shared-types": "^2.9.174",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/uuid": "^9.0.4",
    "@types/zxcvbn": "^4.4.2",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "fs-extra": "^11.1.1",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1",
    "zxcvbn": "^4.4.2"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/basic-auth": "^1.1.4"
  }
}
