{
  "name": "failean-ocserver",
  "version": "2.0.0",
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json",
    "build": "npx ts-node build.ts",
    "update": "npm i",
    "preupdate": "npx npm-check-updates -u",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node ./dist --env=production",
    "predev": "npm install",
    "dev": "nodemon",
    "preudev": "npm run update",
    "udev": "npm run dev",
    "prebuild": "npm run version:patch",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "dependencies": {
    "@failean/mongo-models": "^1.6.119",
    "@failean/shared-types": "^2.9.158",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.2",
    "@types/zxcvbn": "^4.4.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "fs-extra": "^11.1.1",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.1",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "zxcvbn": "^4.4.2"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
